<section style="margin-top: 120px; margin-bottom: 50px">
    <div class="container">
        <form class="form-horizontal" role="form" method="POST" action="/">
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <h2>Bem vindo</h2>
                    <hr>
                </div>
